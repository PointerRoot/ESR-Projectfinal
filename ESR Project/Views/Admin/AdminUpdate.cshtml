
<head>
    <title></title>
    <script type="text/javascript" src="../../Content/ProjectFiles/AdminView/Admins/UpdateAdmins.js"></script>
</head>
<style>
    .blackiconcolor {
        color: red;
    }

        .blackiconcolor:hover {
            color: brown;
        }
</style>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" style="min-height:900px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Admin
            <small>Update</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Admin - Update</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3 id="UsersCounth3Div">@ViewBag.UsersCount</h3>
                        <p>Total Users</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-users"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clients_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-primary">
                    <div class="inner">
                        <h3 id="ClientsCounth3Div">@ViewBag.ClientsCount</h3>
                        <p>Total Clients</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-user"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clietns_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-red">
                    <div class="inner">
                        <h3 id="PakagesCounth3Div">@ViewBag.PakagesCount</h3>
                        <p>Total Pakages</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-file-text "></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Pakages_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>

            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3 id="EventsCounth3Div">@ViewBag.EventsCount</h3>
                        <p>Total Events</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-camera"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Events_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <!-- Add Admin -->
            <section class="col-lg-12 connectedSortable">
                <div id="signupbox" class="mainbox">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <div class="panel-title">Update Admin</div>
                        </div>
                        <div class="panel-body">
                            <form id="Adminform" action="#/" method="post" onsubmit="return UpdateAdminForm()" class="form-horizontal" role="form">
                                <div class="form-group" hidden>
                                    <input type="text" class="form-control" id="AdminId" hidden>
                                </div>
                               
                                <div class="form-group">
                                    <label for="Role" class="col-md-3 control-label">Role</label>
                                    <div class="col-md-9">
                                        <select name="Role" id="AdminRole" class="form-control">
                                            <option value="1">Head</option>
                                            <option value="2">Moderator</option>
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="UserName" class="col-md-3 control-label">UserName*</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="UserName" required id="AdminUserName" pattern="(.){8,50}" placeholder="Alber123: min 8 Character">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Password" class="col-md-3 control-label">Password</label>
                                    <div class="col-md-9">
                                        <input type="password" maxlength="50" class="form-control" pattern="(.){8,50}" name="AdminPassword" id="AdminPassword" placeholder="min-8 characters">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ConfirmPassword" class="col-md-3 control-label">Confirm_Password</label>
                                    <div class="col-md-9">
                                        <input type="password" class="form-control" maxlength="50" name="AdminConfirmPassword" pattern="(.){8,50}" id="AdminConfirmPassword" placeholder="Same As Above">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <!-- Button -->
                                    <div class="col-md-offset-3 col-md-9">
                                        <button id="BtnAdmin" type="submit" class="btn btn-success"><i class="icon-hand-right"></i> &nbsp Submit</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>
            <!-- Add Admin -->
        </div>
    </section>
    <!-- /.content -->
</div>

