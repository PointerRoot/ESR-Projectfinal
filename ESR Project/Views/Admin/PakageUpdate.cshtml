

<head>
    <title></title>
    <script type="text/javascript" src="../../Content/ProjectFiles/AdminView/Pakages/UpdatePakages.js"></script>
</head>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" style="min-height:900px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Pakage</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Pakage</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3 id="UsersCounth3Div">@ViewBag.UsersCount</h3>
                        <p>Total Users</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-users"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clients_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-primary">
                    <div class="inner">
                        <h3 id="ClientsCounth3Div">@ViewBag.ClientsCount</h3>
                        <p>Total Clients</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-user"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clients_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-red">
                    <div class="inner">
                        <h3>@ViewBag.PakagesCount</h3>
                        <p>Total Pakages</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-file-text "></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Pakages_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>

            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>@ViewBag.EventsCount</h3>
                        <p>Total Events</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-camera"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Events_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <!-- Pakage Sign Up -->
            <section class="col-lg-12 connectedSortable">
                <div id="signupbox" class="mainbox">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <div class="panel-title">Update Pakage</div>
                        </div>
                        <div class="panel-body">
                            <form id="Pakageform" action="#/" method="post" onsubmit="return ValidatePakageForm()" class="form-horizontal" role="form">

                                <div class="form-group" hidden>
                                    <div class="col-md-9">
                                        <input type="text" hidden id="PakageId" class="form-control" />
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="Name" class="col-md-3 control-label">Name*</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" maxlength="50" name="Name" required id="Name" placeholder="AlbertPinto">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="icode" class="col-md-3 control-label">Type</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" maxlength="50" name="Type" required id="Type" placeholder="AlbertPinto">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-3 control-label">Services</label>
                                    <div class="col-md-9">
                                        <a href="#/" data-toggle="modal" data-target="#squarespaceModal" id="addTrainerForClass" onclick="$('#loginbox').show()" class=""><i class="fa fa-plus-circle fa-2x" aria-hidden="true"></i></a>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="icode" class="col-md-3 control-label">Cost*</label>
                                    <div class="col-md-9">
                                        <input type="number" class="form-control" name="Price" onkeypress='validate1(event)' required id="Price" placeholder="$120">
                                    </div>
                                </div>


                                <div class="form-group">
                                    <!-- Button -->
                                    <div class="col-md-offset-3 col-md-9">
                                        <button id="BtnPakage" type="submit" class="btn btn-success"><i class="icon-hand-right"></i> &nbsp Submit</button>

                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>

        </div>

        <div class="modal fade" id="squarespaceModal" tabindex="-1" role="dialog" aria-labelledby="modalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content  row">
                    <div class="modal-body">
                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                        <br />

                        <div id="loginbox" class="mainbox col-md-12">
                            <div class="row">
                                <!-- /.col -->
                                <div class="col-md-12">
                                    <div class="box box-primary">
                                        <div class="box-header with-border">
                                            <h3 class="box-title">List</h3>
                                            <div class="box-tools pull-right">
                                                <div class="has-feedback">

                                                </div>
                                            </div>
                                            <!-- /.box-tools -->
                                        </div>
                                        <!-- /.box-header -->
                                        <div class="box-body no-padding">
                                            <div class="mailbox-controls">

                                                <!-- /.pull-right -->
                                            </div>
                                            <div class="table-responsive mailbox-messages">
                                                <table class="table table-hover table-striped">
                                                    <thead>
                                                        <tr>
                                                            <th></th>
                                                            <th>Service Name</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody id="ServiceTable"></tbody>
                                                </table>
                                                <!-- /.table -->
                                            </div>
                                            <!-- /.mail-box-messages -->
                                        </div>
                                        <!-- /.box-body -->
                                        <div class="box-footer no-padding">
                                            <div class="form-group pull-right" style="margin-top:10px">
                                                <!-- Button -->
                                                <div class="col-sm-12 controls pull-right">
                                                    <button id="btnlogin" type="button" onclick="$('#squarespaceModal').modal('hide');" class="btn btn-info"><i class="icon-hand-right"></i>Ok</button>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /. box -->
                                </div>
                                <!-- /.col -->
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

    </section>
    <!-- /.content -->
</div>
