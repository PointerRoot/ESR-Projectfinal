

<head>
    <title></title>
    <script type="text/javascript" src="../../Content/ProjectFiles/AdminView/Contests/UpdateContests.js"></script>
</head>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" style="min-height:900px;">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Contest</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Contest</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3 id="UsersCounth3Div">@ViewBag.UsersCount</h3>
                        <p>Total Users</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-users"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clients_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-primary">
                    <div class="inner">
                        <h3 id="ClientsCounth3Div">@ViewBag.ClientsCount</h3>
                        <p>Total Clients</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-user"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Clients_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-red">
                    <div class="inner">
                        <h3>@ViewBag.PakagesCount</h3>
                        <p>Total Pakages</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-file-text "></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Pakages_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>

            <div class="col-lg-3 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>@ViewBag.EventsCount</h3>
                        <p>Total Events</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa fa-camera"></i>
                    </div>
                    <a href="#/" onclick="myFunctionSwapView('Events_Details');" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
            <!-- Contest Sign Up -->
            <section class="col-lg-12 connectedSortable">
                <div id="signupbox" class="mainbox">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <div class="panel-title">Update Contest</div>
                        </div>
                        <div class="panel-body">
                            <form id="Contestform" action="#/" method="post" onsubmit="return ValidateUpdateForm()" class="form-horizontal" role="form">

                                <div class="form-group" hidden>
                                    <div class="col-md-9">
                                        <input type="text" hidden id="ContestId" class="form-control" />
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-3 control-label">Name*</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" maxlength="50" id="Name" required placeholder="Enter Contest Name Here!">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Entries_Count</label>
                                    <div class="col-md-9">
                                        <input type="number" class="form-control" min="0" onkeypress="OnkeyPressInputInt(event)" onchange="OnChangeInputInt(this)" id="Entries" placeholder="Enter Contest Entries_count Here! : 10">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">WinnerName</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" maxlength="50" id="WinnerName" placeholder="Enter Contest Winner Name Here!">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Reward</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" maxlength="50" id="Reward" placeholder="Enter Contest Reward Here!">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Date</label>
                                    <div class="col-md-9">
                                        <input type="date" class="form-control" onkeypress="OnkeyPressInputInt(event)" id="Date" placeholder="Enter Date Here!">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <!-- Button -->
                                    <div class="col-md-offset-3 col-md-9">
                                        <button id="BtnContest" type="submit" class="btn btn-success"><i class="icon-hand-right"></i> &nbsp Submit</button>

                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </section>

        </div>

    </section>
    <!-- /.content -->
</div>
